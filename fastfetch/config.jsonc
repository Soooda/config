{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"source": "~/.config/fastfetch/ascii/cat.txt",
		"padding": {
			"top": 5,
			"right": 0,
			"bottom": 0,
			"left": 12
		}
	},
	"display": {
		"separator": "",
		"key": {
			"width": 16
		},
	},
	"modules": [
		"break",
		"break",
