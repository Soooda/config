		},
		{
			"key": "┬────────────┬──────────────────────────────────────┤",
			"type": "custom"
		},
		{
			"key": "│                                                   │\u001b[51D{#34}󰌢  {#37}host    {#34}│",
			"type": "title",
			"format": "{2}",
			"outputColor": "34;1"
		},
		{
			"key": "│                                                   │\u001b[51D{#34}󰅐  {#37}uptime  {#34}│ ",
			"type": "uptime",
			"outputColor": "34;1"
